	title = {Distributed no-wait flow shop problem with sequence dependent setup time: Optimization of makespan and maximum tardiness},
	year = {2022},
	journal = {Simulation Modelling Practice and Theory},
	volume = {116},
	doi = {10.1016/j.simpat.2021.102455},
	url = {https://www.scopus.com/inward/record.uri?eid=2-s2.0-85121707133&doi=10.1016%2fj.simpat.2021.102455&partnerID=40&md5=dc345222a2c1bda6e5deea2ffeb4f1c7},
	affiliations = {Laboratory Mathematics, Computer Science and Applications, University Hassan II of Casablanca, FST, PO Box 146, Mohammedia, Morocco; National Higher School Of Arts And Crafts (ENSAM), University Hassan II of Casablanca, Casablanca, 20670, Morocco},
	abstract = {This paper investigates a multi-objective optimization distributed no-wait permutation flow shop scheduling problem under the constraint of sequence dependent setup time. Our optimization case is minimizing the makespan and maximum tardiness criteria. Therefore, our main objective will be to find the optimal jobs sequence that minimizes a function representing the two criteria of makespan and maximum tardiness. This function will be linearly dependent of these two criteria via a weighting parameter for each criterion. To solve this industrial problem, we propose the mixed integer linear programming (MILP) and a set of efficient metaheuristics solving different size instances. To this end, we suggest three inspired nature metaheuristics: The genetic algorithm (GA), the artificial bee colony (ABC) algorithm and migratory bird optimization (MBO) algorithm. We suggest a total of six new algorithms based on nature-inspired metaheuristics. Also, two constructive heuristics are used, the greedy randomized adaptive search procedure (GRASP) and Nawaz–Enscore–Ham (NEH) algorithms. It was revealed that GA algorithm with NEH initialization gives the best results comparing to the other metaheuristics. © 2021 Elsevier B.V.},
	author_keywords = {Distributed no-wait flow shop; Inspired nature metaheuristics; Makespan and maximum tardiness; Sequence dependent setup time},
	keywords = {Biomimetics; Genetic algorithms; Integer programming; Machine shop practice; Multiobjective optimization; Distributed no-wait flow shop; Flow shop problems; Inspired nature metaheuristic; Makespan; Makespan and maximum tardiness; Maximum tardiness; Metaheuristic; No-wait flowshop; Sequence-dependent setup time; Setup time optimization; Heuristic algorithms},
	correspondence_address = {K. Allali; Laboratory Mathematics, Computer Science and Applications, University Hassan II of Casablanca, Mohammedia, FST, PO Box 146, Morocco; email: karam.allali@fstm.ac.ma},
	publisher = {Elsevier B.V.},
	issn = {1569190X},
	language = {English},
	abbrev_source_title = {Simul. Model. Pract. Theory},
	type = {Article},
	publication_stage = {Final},
	source = {Scopus},
	note = {Cited by: 29}
}

