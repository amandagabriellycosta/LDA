	affiliations = {Universiti Kebangsaan Malaysia, Faculty of Information Science and Technology, Bangi, 43600, Malaysia; Universiti Kebangsaan Malaysia, Center for Artificial Intelligence Technology, Bangi, 43600, Malaysia; Universiti Kebangsaan Malaysia Medical Centre, Faculty of Medicine, Kuala Lumpur, 56000, Malaysia; Universiti Kebangsaan Malaysia, Faculty of Medicine, Department of Anaesthesiology and Intensive Care, Kuala Lumpur, 56000, Malaysia},
