	keywords = {Energy efficiency; Heuristic algorithms; Learning algorithms; Radio; Resource allocation; Scheduling; Scheduling algorithms; Coordinated scheduling; Efficient resource allocation; Exhaustive search algorithms; Formulated problems; Greedy algorithms; Near-optimal; Optimization problems; Radio access networks; Optimization},
